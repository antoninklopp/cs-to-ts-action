{
  "name": "cs-to-ts-action",
  "version": "1.0.0",
  "private": true,
  "description": "Action to check if c# and ts models match",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoninklopp/cs-to-ts-action.git"
  },
  "keywords": [
    "ts",
    "cs",
    "c#",
    "action"
  ],
  "author": "antoninklopp",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.0.0",
    "@actions/github": "^1.0.0",
    "@octokit/rest": "^16.28.7",
    "eslint": "^6.8.0",
    "js-yaml": "^3.13.1",
    "minimatch": "^3.0.4",
    "semver": "^6.1.1",
    "vscode": "^1.1.36"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/js-yaml": "^3.12.1",
    "@types/minimatch": "^3.0.0",
    "@types/node": "^12.0.4",
    "@types/semver": "^6.0.0",
    "eslint-plugin-github": "^3.4.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-circus": "^24.7.1",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build && npm run format"
    }
  }
}
